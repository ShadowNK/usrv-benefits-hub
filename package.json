{
  "name": "usrv-benefits-hub",
  "version": "1.0.0",
  "description": "usrv-benefits-hub",
  "main": "handler.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test:unit"
    }
  },
  "scripts": {
    "lint": "npm run lint:format && npm run lint:duplicate && npm run lint:check",
    "lint:check": "npx tslint -t stylish --project tsconfig.json -c tslint.json 'src/**/*.ts' && npm run tsc:check",
    "lint:duplicate": "npx jscpd ./src",
    "lint:fix": "npx tslint -t stylish --fix --project tsconfig.json -c tslint.json 'src/**/*.ts'",
    "lint:format": "npx prettier --write \"./**/*.{ts,js,json,yml,yaml}\" \"./.{kushkirc,nycrc,prettierrc}\"",
    "test": "npm run test:unit && npm run test:coverage",
    "test:coverage": "npx nyc mocha --config ./.mocharc.nyc.yml src/**/*.spec.ts",
    "test:watch": "npx mocha --watch src/**/*.spec.ts",
    "test:unit": "npx mocha src/**/*.spec.ts",
    "tsc:interface": "npx kushki json2ts",
    "tsc:check": "npx tsc --project tsconfig.check.json --noEmit",
    "validate": "npm run depcheck && npm run lint && npm run test",
    "depcheck": "npx depcheck . --ignores=\"serverless-*,@types/*,@kushki/*,prettier,tsutils,chai,husky,serverless,rollbar,aws-lambda,lint-staged,ajv,ts-loader,tslint-config-prettier,infrastructure,constant,middleware,core,gateway,service,repository,types,sinon,aws-sdk,ts-mockery,cache-loader, source-map-loader,thread-loader\""
  },
  "devDependencies": {
    "@kushki/cli": "1.4.1",
    "@kushki/nyc": "1.0.3",
    "@kushki/tsconfig": "1.1.0",
    "@kushki/tslint": "5.2.12",
    "@types/aws-lambda": "8.10.61",
    "@types/chai": "4.2.12",
    "@types/chai-as-promised": "7.1.3",
    "@types/chai-json-schema": "1.4.6",
    "@types/lodash": "4.14.168",
    "@types/mocha": "8.0.2",
    "@types/mockery": "1.4.29",
    "@types/node": "14.0.27",
    "@types/proxyquire": "1.3.28",
    "@types/request-promise": "4.1.46",
    "@types/sinon": "9.0.4",
    "@types/sinon-chai": "3.2.4",
    "@types/underscore": "1.10.21",
    "cache-loader": "4.1.0",
    "chai": "4.2.0",
    "copy-webpack-plugin": "6.0.3",
    "depcheck": "1.2.0",
    "husky": "4.2.5",
    "jscpd": "3.3.13",
    "lint-staged": "10.2.11",
    "mocha": "8.1.1",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "serverless": "1.78.1",
    "serverless-apigw-binary": "0.4.4",
    "serverless-cloudformation-resource-counter": "1.0.3",
    "serverless-domain-manager": "4.2.0",
    "serverless-dynamodb-autoscaling": "0.6.2",
    "serverless-enable-api-logs": "2.0.0",
    "serverless-localstack": "^0.4.24",
    "serverless-offline": "6.5.0",
    "serverless-plugin-aws-alerts": "^1.6.1",
