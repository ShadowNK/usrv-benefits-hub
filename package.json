    "serverless-webpack": "5.3.3",
    "sinon": "9.0.3",
    "source-map-loader": "1.0.1",
    "terser-webpack-plugin": "4.1.0",
    "thread-loader": "2.1.3",
    "ts-loader": "8.0.2",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tsutils": "3.17.1",
    "typescript": "3.9.7",
    "uuid": "8.3.2",
